{
    "manifest_version": 3,
    "name": "Fextralife VIP Features",
    "description": "Enables Fextralife VIP features without having to pay it",
    "version": "1.0",
    "content_scripts": [
        {
            "js": ["scripts/patcher.js"],
            "all_frames": true,
            "matches": [
                "https://eldenring.wiki.fextralife.com/file/Elden-Ring/map-*"
            ],
            "world": "MAIN"
        }
    ],
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    }
}
